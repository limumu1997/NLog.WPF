<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>disable</Nullable>
		<UseWPF>true</UseWPF>
		<FileVersion>1.2.4.0</FileVersion>
		<AssemblyVersion>1.2.4.0</AssemblyVersion>
		<Version>1.2.4</Version>
		<Authors>limumu1997</Authors>
		<Company>iliotdotcn</Company>
		<PackageDescription>NLog.WPF</PackageDescription>
		<RepositoryUrl>https://github.com/limumu1997/NLog.WPF</RepositoryUrl>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>NLog.WPF is a simple WPF-control to show NLog-logs. </PackageTags>
		<PackageProjectUrl>https://github.com/limumu1997/NLog.WPF</PackageProjectUrl>
		<Description>NLog.WPF is a simple WPF-control to show NLog-logs. </Description>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="NLog" Version="5.2.2" />
	</ItemGroup>

</Project>
